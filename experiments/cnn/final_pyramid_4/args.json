{
  "arch": "PyramidCNN",
  "data": "256x256-split",
  "epochs": 100,
  "lr": 0.0001,
  "momentum": 0.9,
  "no_cuda": false,
  "no_augment": false,
  "optimizer": "Adam",
  "batch_size": 32,
  "criterion": "label-smooth",
  "smooth_criterion": 0.1,
  "early_stop": 6,
  "weight_decay": 0.001,
  "kernel_size": 3,
  "dropout": 0.25,
  "no_batch_norm": false,
  "conv_layers": 2,
  "conv_blocks": 5,
  "fc_layers": 2,
  "initial_channels": 16,
  "no_pool": false,
  "autoencoder": false,
  "encoder_dims_out": 8,
  "encoder_channels_out": 256
}